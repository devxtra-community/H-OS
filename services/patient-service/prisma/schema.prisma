generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Patient {
  id        String   @id @default(uuid())
  firstName String
  lastName  String
  email     String   @unique
  phone     String?
  dob       DateTime?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
